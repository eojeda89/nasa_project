{
  "name": "nasa-project-api",
  "version": "1.0.0",
  "private": true,
  "description": "NASA Mission Control API",
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd clients && npm start",
    "watch": "npm run server & npm run client",
    "deploy": "npm run build --prefix clients && npm run server",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.0.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
